<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:context="com.faishalbadri.hijab.aaa_migration_server.ui.sponsor.SponsorActivity">

  <include
    android:id="@+id/toolbar_activity_sponsor"
    layout="@layout/general_toolbar_with_back_button"/>

  <android.support.v7.widget.RecyclerView
    android:id="@+id/recyclerview_activity_sponsor"
    android:layout_width="0dp"
    android:layout_height="0dp"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintLeft_toLeftOf="parent"
    app:layout_constraintRight_toRightOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/toolbar_activity_sponsor"/>

</android.support.constraint.ConstraintLayout>
